{
  "name": "react-math-view",
  "version": "1.4.0",
  "description": "React bindings for MathLive",
  "author": "ShaMan123",
  "license": "MIT",
  "repository": "ShaMan123/react-math-view",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "workspaces": [
    "example"
  ],
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "example:start": "cd example && yarn start",
    "dev": "yarn example:start && yarn start",
    "prepare": "run-s build",
    "test": "run-s test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "predeploy": "cd example && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=17.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^20.1.4",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^5.0.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "mathlive": "^0.93.0"
  }
}
